    "lint": "eslint --ext .tsx,.ts .",
    "lint:fix": "pnpm lint -- --fix"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
