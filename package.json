{
  "name": "react-ts-tail-jest",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.1.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest --no-watchman",
    "test:watch": "jest",
    "test:coverage": "jest --no-watchman --coverage",
